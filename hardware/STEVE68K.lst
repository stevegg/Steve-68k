 10:
 11:/* *************** INPUT PINS *********************/
 12:PIN   1 = CLK;          /* CLK */
 13:PIN   2 = A19;          /* Address 19 */
 14:PIN   3 = A20;          /* Address 20 */
 15:PIN   4 = A21;          /* Address 21 */
 16:PIN   5 = AS;                   /* Address Strobe */
 17:PIN   6 = DTACK_UART;   /* UART DTACK Output */
 18:PIN   7 = DTACK_I2C;            /* I2C DTACK Output */
 19:PIN   8 = DTACK_USER;   /* User DTACK output */
 20:PIN   9 = USERSEL;              /* When LOW allows user selected DTACK */
 21:
 22:/* *************** OUTPUT PINS *********************/
 23:PIN  12 = ROMSEL;               /* When Low selects ROM */
 24:PIN  13 = RAMSEL;               /* When Low selects RAM */
 25:PIN  14 = UARTSEL;              /* When Low selects UART */
 26:PIN  15 = I2CSEL;               /* When Low selects I2C */
 27:PIN  16 = DTACK;                /* DTACK output to the CPU */
 28:PIN  17 = ERR;          /* When an out of range address is selected */
 29:
 30:
 31:/* IO Decoding.  LOTS of wasted space */
 32:field ioaddr = [A19, A20, A21];
 33:ROMeqn = ioaddr:[0..7FFFF];             /* 512k of ROM */
 34:RAMeqn = ioaddr:[80000..9FFFF]; /* 512k of RAM */
 35:UARTeqn = ioaddr:[100000..17FFFF];      /* 512k of UART....LOL */
 36:I2Ceqn = ioaddr:[180000..180050];       /* I2C Select */
 37:ERReqn = ioaddr:[380000..385000]; /* Out of range address */
 38:
 39:ROMSEL=!ROMeqn;
 40:RAMSEL=!RAMeqn;
 41:UARTSEL=!UARTeqn;
 42:I2CSEL=!I2Ceqn;
 43:ERR=!ERReqn;
 44:
 45:/*
 46:DTACK=(!USERSEL&DTACK_USER)
 47:    # ((ROMeqn#RAMeqn)&AS)
 48:    # (UARTeqn&DTACK_UART)
 49:    # (I2Ceqn&DTACK_I2C);
 50:*/
 51:
 52:DTACK=(!USERSEL&DTACK_USER)
 53:    # ((ROMeqn#RAMeqn)&AS)

